<html>

<head>
    <title>EXP</title>
    <link rel="stylesheet" type="text/css" href="css/experimento.css">

</head>

<body style="overflow: hidden;">

    <div class="container">
        <form action='/envExpA' method='POST'>
            <button type="submit" onclick='clickA();' class="botao" id="opA">A</button>
        </form>
    </div>

    <div class="container2">
        <form action='/envExpB' method='POST'>
            <button type="submit" onclick='clickB();' class="botao" id="opB">B</button>
        </form>
    </div>

    <!-- <form action='/envExpBComp' name="envExpBComp" method='POST'/> -->

</body>

</html>

<script type="text/javascript">
    var tempo = 0;
    function clickA() {
        console.log("opA");
        console.log(tempo);
        document.getElementById("opA").style.display  = "none";
        document.getElementById("opB").style.display  = "none";
        return apareceA();
    }
    function clickB(){
        console.log("opB");
        console.log(tempo);
        document.getElementById("opA").style.display  = "none";
        document.getElementById("opB").style.display  = "none";
        return apareceB();
    }
    function apareceA(){
        setTimeout(function(){  
            document.getElementById("opA").style.display  = "block";
        }, 10 * 1000)
    }
    function apareceB(){
        setTimeout(function(){
            document.getElementById("opB").style.display  = "block";
            // envFormB();
        }, ("<%= atrasoB %>"  * 1000) + (10 * 1000) )
    }
    // function envFormB(){
